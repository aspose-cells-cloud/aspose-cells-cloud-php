{
	"name" : "aspose/cells-sdk-php",
	"type" : "library",
	"description" : "A PHP library for communicating with the Aspose.Cells Cloud API",
	"keywords" : [
		"aspose",
		"php",
		"aspose cloud"
	],
	"homepage" : "https://github.com/aspose-cells-cloud/aspose-cells-cloud-php",
	"license" : "MIT",
	"authors" : [{
			"name" : "aspose-cells-cloud",
			"homepage" : "https://github.com/aspose-cells-cloud/aspose-cells-cloud-php"
		}
	],
	"require" : {
		"php" : ">=5.5",
		"ext-curl" : "*",
		"ext-json" : "*",
		"ext-mbstring" : "*",
		"guzzlehttp/guzzle" : "^7.4"
	},
	"require-dev" : {
		"phpunit/phpunit" : "^4.8",
		"squizlabs/php_codesniffer" : "~2.6"
	},
	"autoload": {
        "psr-4": { "Aspose\\Cells\\Cloud\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "Aspose\\Cells\\Cloud\\" : "test/" }
  }
}